<div id="content_colors" class="content">
  <div class="list">
    <img class="screen-preview" id="screen" src="pic/screen.jpg" alt="TV screen">
  </div>
  <br>
  <div class="list">
    <div class="screen-preview" id="led.color"></div>
  </div>
  <br>
  led.brightness
  <div class="list">
    <input class="slider" id="led.brightness" type="range" min="1" max="100" onchange="saveSettings()">
  </div>
  <br>
  screen.x
  <div class="list">
    <input class="slider" id="screen.x" type="range" min="0" max="640" onchange="saveSettings()">
  </div>
  screen.y
  <div class="list">
    <input class="slider" id="screen.y" type="range" min="0" max="480" onchange="saveSettings()">
  </div>
  screen.w
  <div class="list">
    <input class="slider" id="screen.w" type="range" min="0" max="640" onchange="saveSettings()">
  </div>
  screen.h
  <div class="list">
    <input class="slider" id="screen.h" type="range" min="0" max="480" onchange="saveSettings()">
  </div>
</div>
